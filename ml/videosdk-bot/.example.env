VIDEOSDK_AUTH_TOKEN=
GROQ_API_KEY=
CARTESIA_API_KEY=

# Agent Mode: assistant, interview, chatbot
AGENT_MODE=assistant
AGENT_LANGUAGE=ru
AGENT_MAX_QUESTIONS=12

# STT Settings
STT_MODEL=whisper-large-v3-turbo
STT_SILENCE_THRESHOLD=0.05
STT_SILENCE_DURATION=1.2

# Cartesia STT Settings
USE_CARTESIA_STT=true
CARTESIA_STT_MODEL=ink-whisper
CARTESIA_STT_LANGUAGE=ru

# LLM Settings  
LLM_MODEL=qwen/qwen3-32b
LLM_TEMPERATURE=0.7
# LLM_MAX_TOKENS=1024

# TTS Settings
TTS_MODEL=playai-tts

# Cartesia TTS Settings
USE_CARTESIA_TTS=true
CARTESIA_MODEL=sonic-2
CARTESIA_VOICE=english_female_1
CARTESIA_LANGUAGE=en

# VAD Settings (для хорошего перебивания)
VAD_THRESHOLD=0.3
VAD_MIN_SPEECH_DURATION=0.3
VAD_MIN_SILENCE_DURATION=0.8

# Turn Detector Settings (для хорошего перебивания)
TURN_DETECTOR_THRESHOLD=0.5

# Interview Mode Settings
USE_INTERVIEW_MODE=true
# Predefined configs: python_developer, frontend_developer, fullstack_developer, data_scientist, devops_engineer
INTERVIEW_CONFIG=python_developer
# Custom config (используется если INTERVIEW_CONFIG не найден в предустановленных)
INTERVIEW_POSITION=Middle Python Developer
INTERVIEW_COMPANY=TechCompany
INTERVIEW_MAX_QUESTIONS=12
INTERVIEW_SAVE_DIR=./interview_results
INTERVIEW_JOB_DESCRIPTION=Мы ищем опытного Python разработчика для работы над веб-приложениями и API. Основные задачи: разработка веб-приложений на Django/FastAPI, проектирование и разработка REST API, работа с базами данных (PostgreSQL, Redis), написание тестов и поддержка CI/CD, код-ревью и менторство. Технологии: Python 3.8+, Django/FastAPI, PostgreSQL, Redis, Docker, Git

# Legacy Interview Settings
INTERVIEW_STYLE=доброжелательный, профессиональный, нейтральный
INTERVIEW_END_MARKER=__END_INTERVIEW__
AUTO_SAVE_INTERVIEWS=true
INTERVIEWS_SAVE_PATH=interviews/

# Room Settings
# ROOM_ID=your_room_id_to_join_existing
PLAYGROUND_MODE=true

# Component Selection
USE_INTEGRATED_COMPONENTS=false